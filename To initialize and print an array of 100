#include <stdio.h>
int main()
{
    int i;
    int a[100];
    
    printf("Enter the values:");
    for(i=0;i<100;i++)
    {
        if(i<30)
        a[i]=1;
        else a[i]=0;
        //scanf("%d", &a[i]);
    }
    
    printf("the values you entered:");
    for(i=0;i<32;i++)
    {
        printf("%d\n",a[i]);
    }
    return 0;
}
