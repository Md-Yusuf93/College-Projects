#include <stdio.h>
void main()
{
    int i;
    int a[10];
    int even=0,odd=0;
    
    for(i=0;i<10;i++)
    {
        printf("Enter the value:");
        scanf("%d",&a[i]);
    }
    for(i=0;i<10;i++)
    {
        if(a[i]%2==0)
        even = even+1;
        else
        odd = odd+1;
    }
    printf("even:%d\n",even);
    printf("odd:%d\n",odd);
    
}
